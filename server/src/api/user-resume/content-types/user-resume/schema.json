  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "resumeId": {
      "type": "string"
    },
    "userEmail": {
      "type": "email"
    },
    "userName": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "jobTitle": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "summery": {
      "type": "string"
    },
    "education": {
      "type": "component",
      "repeatable": true,
      "component": "education.education"
    },
    "skills": {
      "type": "component",
      "repeatable": true,
      "component": "skills.skills"
    },
    "experience": {
      "type": "component",
      "repeatable": true,
      "component": "experience.experience"
    },
    "themeColor": {
      "type": "string"
    },
