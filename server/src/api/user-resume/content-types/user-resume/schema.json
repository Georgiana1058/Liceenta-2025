      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "linkedin": {
      "type": "text"
    },
    "github": {
      "type": "text"
    },
    "volunteering": {
      "type": "component",
      "repeatable": true,
      "component": "volunteering.volunteering"
    },
    "languages": {
      "type": "component",
      "repeatable": true,
      "component": "languages.languages"
    },
    "certificates": {
      "type": "component",
      "repeatable": true,
      "component": "certificates.certificates"
    },
    "appreovedCVs": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cv.cv",
      "mappedBy": "soucerResume"
    },
    "isApproved": {
      "type": "boolean"
    },
    "ratingResume": {
      "type": "integer"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "user_resume"
    },
    "portofolio": {
      "type": "component",
      "repeatable": true,
      "component": "portofolio.portofolio"
    },
    "certificateUrl": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
